<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="expressionPatternMatchers" name="%expPatternMatcherID" schema="schema/expressionPatternMatchers.exsd"/>
   <extension-point id="predicatePatternMatchers" name="%predPatternMatcherID" schema="schema/predicatePatternMatchers.exsd"/>
   <extension
         point="org.eventb.core.seqprover.reasoners">
      <reasoner
            class="org.eventb.theory.rbp.reasoners.ManualRewriteReasoner"
            id="manualRewriteReasoner"
            name="%manualRewriteReasoner">
      </reasoner>
      <reasoner
            class="org.eventb.theory.rbp.reasoners.AutoRewriteReasoner"
            id="autoRewriteReasoner"
            name="%autoRewriteReasoner">
      </reasoner>
      <reasoner
            class="org.eventb.theory.rbp.reasoners.AutoInferenceReasoner"
            id="autoInferenceReasoner"
            name="%autoInferenceReasoner">
      </reasoner>
      <reasoner
            class="org.eventb.theory.rbp.reasoners.ManualInferenceReasoner"
            id="manualInferenceReasoner"
            name="%manualInferenceReasoner">
      </reasoner>
   </extension>
   <extension
         point="org.eventb.core.seqprover.autoTactics">
      <autoTactic
            class="org.eventb.theory.rbp.tactics.RewritesAutoTactic"
            description="%rbp0.desc"
            id="rbp0"
            name="%rbp0">
      </autoTactic>
      <autoTactic
            class="org.eventb.theory.rbp.tactics.InferenceAutoTactic"
            description="%rbp1.desc"
            id="rbp1"
            name="%rbp1">
      </autoTactic>
   </extension>
   <extension
         point="org.eventb.core.pomTactics">
      <tactic
            id="org.eventb.theory.rbp.rbp0">
      </tactic>
      <tactic
            id="org.eventb.theory.rbp.rbp1">
      </tactic>
   </extension>
   <extension
         point="org.eventb.core.postTactics">
      <tactic
            id="org.eventb.theory.rbp.rbp0">
      </tactic>
      <tactic
            id="org.eventb.theory.rbp.rbp1">
      </tactic>
   </extension>
   <extension
         point="org.eventb.ui.proofTactics">
      <tactic
            icon="icons/pending.gif"
            id="RbP0g"
            interrupt="true"
            name="%rbpRewTactics"
            priority="5"
            tacticProvider="org.eventb.theory.rbp.tactics.RewritesManualTactic"
            target="goal"
            tooltip="%applyRew">
      </tactic>
       <tactic
            icon="icons/pending.gif"
            id="RbP0h"
            interrupt="true"
            name="%rbpRewTactics"
            priority="5"
            tacticProvider="org.eventb.theory.rbp.tactics.RewritesManualTactic"
            target="hypothesis"
            tooltip="%applyRew">
      </tactic>
      <tactic
            icon="icons/infer.gif"
            id="RbP1g"
            interrupt="true"
            name="%rbpInfTactics"
            priority="5"
            tacticProvider="org.eventb.theory.rbp.tactics.InferenceManualTactic"
            target="goal"
            tooltip="%applyInf">
      </tactic>
       <tactic
            icon="icons/infer.gif"
            id="RbP1h"
            interrupt="true"
            name="%rbpInfTactics"
            priority="5"
            tacticProvider="org.eventb.theory.rbp.tactics.InferenceManualTactic"
            target="hypothesis"
            tooltip="%applyInf">
      </tactic>
   </extension>
   <extension
         point="org.eventb.theory.rbp.expressionPatternMatchers">
