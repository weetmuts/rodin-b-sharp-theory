         <activeWhen>
            <and>
               <with
                     variable="activeEditorId">
                  <equals
                        value="org.eventb.theory.ui.theoryEditor">
                  </equals>
               </with>
               <with
                     variable="activeFocusControlId">
                   <or>
                     <equals
                           value="org.eventb.ui.controls.text">
                     </equals>
                     <equals
                           value="org.eventb.ui.controls.styledText">
                     </equals>
                  </or>
               </with>
            </and>
          </activeWhen>
      </handler>
